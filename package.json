{
  "name": "proshop",
  "version": "1.0.0",
  "description": "MERN shopping cart app",
  "main": "server.js",
  "type": "module",
  "scripts": {
    "dev": "node backend/server",
    "server": "nodemon backend/server",
    "client": "npm start --prefix frontend",
    "start": "concurrently \"npm run client\" \"npm run dev\"",
    "data:import": "node backend/seeder",
    "data:destroy": "node backend/seeder -d",
    "build":"cd frontend/ npm install && npm build"
  },
  "author": "George Gitau",
  "license": "MIT",
  "dependencies": {
    "autoprefixer": "10.4.5",
    "bcryptjs": "^2.4.3",
    "colors": "^1.4.0",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "express-async-handler": "^1.2.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^6.4.1"
  },
  "devDependencies": {
    "concurrently": "^7.2.2",
    "nodemon": "^2.0.18"
  }
}
